{
  "name": "nitsan/ns-headless-mask",
  "type": "typo3-cms-extension",
  "description": "Support headless for the mask elements",
  "keywords": [
    "TYPO3",
    "headless",
    "pwa",
    "json",
    "mask"
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
   "support": {
    "issues": "https://github.com/nitsan-technologies/ns_headless_mask/issues"
  },
  "require": {
    "typo3/cms-core": "^12 || ^13"
  },
  "suggest": {
  	"friendsoftypo3/headless": "^4.0",
    "mask/mask": "^8 || ^9"
  },
  "extra": {
     "typo3/cms": {
        "extension-key": "ns_headless_mask"
     }
  },
  "autoload": {
    "psr-4": {
      "NITSAN\\NsHeadlessMask\\": "Classes"
    }
  }
}
